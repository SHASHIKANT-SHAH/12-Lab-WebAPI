      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebAPIDBOperations
      target: base
    depends_on:
      dboperationapp:
        condition: service_started
        required: true
    entrypoint:
      - dotnet
      - --roll-forward
      - Major
      - /VSTools/DistrolessHelper/DistrolessHelper.dll
      - --wait
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DB_HOST: dboperationapp
      DB_NAME: apidbname
      DB_SA_PASSWORD: yourStrong(!)Password
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
    image: shashikantshah951/webapidboperations:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net6.0/WebAPIDBOperations.dll"'
