services:
  dboperationapp:
    container_name: db-operation-app
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: yourStrong(!)Password
    image: mcr.microsoft.com/mssql/server:2019-CU14-ubuntu-20.04
    networks:
      api_network: null
    ports:
      - mode: ingress
        target: 1433
        published: "5061"
        protocol: tcp
  webapidboperations:
